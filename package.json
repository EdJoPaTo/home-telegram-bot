{
	"name": "home-telegram-bot",
	"private": true,
	"version": "2.2.0",
	"license": "MIT",
	"repository": "EdJoPaTo/home-telegram-bot",
	"author": {
		"name": "EdJoPaTo",
		"email": "home-telegram-bot-tgbot@edjopato.de",
		"url": "https://edjopato.de"
	},
	"scripts": {
		"build": "rm -rf dist && tsc",
		"start": "ts-node source/index.ts",
		"test": "tsc --sourceMap && xo && nyc ava"
	},
	"type": "commonjs",
	"engines": {
		"node": ">=14"
	},
	"dependencies": {
		"@sindresorhus/tsconfig": "^2.0.0",
		"@types/d3": "^6.0.0",
		"@types/debounce-promise": "^3.1.3",
		"@types/json-stable-stringify": "^1.0.32",
		"@types/node": "^14.17.9",
		"@types/sharp": "^0.29.2",
		"array-filter-unique": "^2.0.0",
		"async-mqtt": "^2.0.0",
		"ava": "^3.5.0",
		"d3": "^6.0.0",
		"d3-node": "^2.1.0",
		"debounce-promise": "^3.1.0",
		"json-stable-stringify": "^1.0.1",
		"sharp": "^0.29.0",
		"source-map-support": "^0.5.19",
		"telegraf": "^4.3.0",
		"telegraf-inline-menu": "^6.1.0",
		"telegraf-session-local": "^2.0.0",
		"telegraf-stateless-question": "^2.0.0",
		"telegram-format": "^2.0.1",
		"typescript": "^4.2.4"
	},
	"devDependencies": {
		"nyc": "^15.0.0",
		"ts-node": "^10.0.0",
		"xo": "^0.44.0"
	},
	"nyc": {
		"all": true,
		"reporter": [
			"lcov",
			"text"
		]
	},
	"xo": {
		"rules": {
			"@typescript-eslint/no-require-imports": "off",
			"@typescript-eslint/no-unsafe-assignment": "off",
			"@typescript-eslint/no-unsafe-call": "off",
			"@typescript-eslint/no-unsafe-member-access": "off",
			"@typescript-eslint/no-unsafe-return": "off",
			"ava/no-ignored-test-files": "off",
			"node/prefer-global/process": "off",
			"unicorn/prefer-module": "off",
			"unicorn/prefer-node-protocol": "off"
		}
	}
}
